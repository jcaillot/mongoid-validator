{
  "name": "chaman/mongoid",
  "type": "library",
  "description": "Symfony validator for mongoid string",
  "keywords": [
    "Mongo",
    "Symfony",
    "Validator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "jerome caillot",
      "email": "jerome@chaman.ca"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-mongodb": "~1.4",
    "symfony/validator": "^4.4 || ^5.0"
  },
  "require-dev": {
    "phpstan/phpstan": "^0.12.98",
    "phpunit/phpunit": "^9.5",
    "symfony/phpunit-bridge": "^5.3"
  },
  "autoload": {
    "psr-4": {
      "Chaman\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Chaman\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
